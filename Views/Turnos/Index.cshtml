@model IEnumerable<AgendaDelConsultorio.Models.turno>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FechaTurno)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HoraTurno)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Observacion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Especialidad)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EstadoTurno)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Paciente)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FechaTurno)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HoraTurno)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Observacion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Especialidad.EspecialidadId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EstadoTurno.EstadoTurnoId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Paciente.PacienteId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.TurnoId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.TurnoId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.TurnoId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
